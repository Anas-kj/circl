{
  "plugins": [],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "prefer-const": "off",
    "@typescript-eslint/no-unused-vars": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/no-var-requires": 0,
    "@next/next/no-img-element": "off"
  },
  "settings": {
    "next": {
      "rootDir": "/packages/*/"
    }
  }
}
